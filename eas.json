
{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_NO_METRO_LAZY": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_NO_METRO_LAZY": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_NO_METRO_LAZY": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
